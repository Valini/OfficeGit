<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>com.jac.web.controller.LoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EmployeeController</servlet-name>
  	<display-name>EmployeeController</display-name>
  	<description></description>
  	<servlet-class>com.jac.web.controller.EmployeeController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BookController</servlet-name>
  	<display-name>BookController</display-name>
  	<description></description>
  	<servlet-class>com.jac.web.controller.BookController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>New</servlet-name>
  	<display-name>New</display-name>
  	<description></description>
  	<servlet-class>com.jac.web.controller.New</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InventoryController</servlet-name>
  	<display-name>InventoryController</display-name>
  	<description></description>
  	<servlet-class>com.jac.web.controller.InventoryController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditEmployeeController</servlet-name>
  	<display-name>EditEmployeeController</display-name>
  	<description></description>
  	<servlet-class>com.jac.web.controller.EditEmployeeController</servlet-class>
  </servlet>

  <servlet>
  	<servlet-name>LibraryController</servlet-name>
  	<display-name>LibraryController</display-name>
  	<description></description>
  	<servlet-class>com.jac.web.controller.LibraryController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ManageController</servlet-name>
  	<display-name>ManageController</display-name>
  	<description></description>
  	<servlet-class>com.jac.web.controller.ManageController</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EmployeeController</servlet-name>
  	<url-pattern>/EmployeeController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BookController</servlet-name>
  	<url-pattern>/bookSearch</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>New</servlet-name>
  	<url-pattern>/New</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InventoryController</servlet-name>
  	<url-pattern>/InventoryController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditEmployeeController</servlet-name>
  	<url-pattern>/EditEmployee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LibraryController</servlet-name>
  	<url-pattern>/LibraryController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ManageController</servlet-name>
  	<url-pattern>/Manage</url-pattern>
  </servlet-mapping>

    <welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
